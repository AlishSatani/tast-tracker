{
  "name": "ts-express",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon -r dotenv-expand/config src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node -r dotenv-expand/config dist/index.js",
    "setup:db": "node -r dotenv-expand/config ./scripts/setup_db.mjs && yarn db reset --erase && yarn db reset --erase --shadow",
    "prod:start": "yarn db migrate && yarn start",
    "db": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS -r dotenv-expand/config\" graphile-migrate",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "Express"
  ],
  "author": "Alish Satani",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "inquirer": "^9.1.5",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@graphile-contrib/pg-many-to-many": "^1.0.2",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "graphile-migrate": "^1.4.1",
    "graphile-worker": "^0.16.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "postgraphile": "^4.12.12",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "postgraphile-plugin-nested-mutations": "^1.1.0",
    "yarn-run-all": "^3.1.1"
  }
}
